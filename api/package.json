{
  "name": "uniinsider",
  "version": "1.0.0",
  "description": "## Setup",
  "main": "index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/index.js",
    "start:prod": "cross-env NODE_ENV=production nodemon src/index.js",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "prepare": "husky install",
    "db:create": "cross-env NODE_ENV=development sequelize-cli db:create",
    "db:migrate": "cross-env NODE_ENV=development sequelize-cli db:migrate",
    "db:seeds": "cross-env NODE_ENV=development sequelize-cli db:seed:all",
    "db:g:seed": "cross-env NODE_ENV=development sequelize-cli seed:generate --name"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.26.1",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.0",
    "enum": "3.0.4",
    "excel4node": "^1.8.0",
    "exceljs": "^4.3.0",
    "express": "4.18.1",
    "express-fileupload": "^1.4.0",
    "express-rate-limit": "6.3.0",
    "helmet": "5.0.2",
    "http-status": "1.5.0",
    "joi": "17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "1.10.0",
    "multer": "^1.4.4-lts.1",
    "nodemailer": "^6.9.1",
    "passport": "0.5.2",
    "pg": "8.7.3",
    "pg-hstore": "2.3.4",
    "pm2": "5.2.0",
    "prettier": "2.6.2",
    "sequelize": "6.18.0",
    "sequelize-cli": "6.4.1",
    "swagger-jsdoc": "6.2.0",
    "swagger-ui-express": "4.3.0",
    "uuidv4": "6.2.13",
    "winston": "3.7.2",
    "xlsx": "^0.18.5",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "eslint": "8.13.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-security": "1.4.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "nodemon": "2.0.15"
  }
}
